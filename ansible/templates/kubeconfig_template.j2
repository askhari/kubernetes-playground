apiVersion: v1
clusters:
- cluster:
    certificate-authority-data: {{ k8s_user_certificate }}
    server: {{ k8s_server_url }}
  name: {{ k8s_cluster_name }}
contexts:
- context:
    cluster: {{ k8s_cluster_name }}
    user: {{ k8s_username }}
  name: {{ k8s_cluster_name }}
current-context: {{ k8s_cluster_name }}
kind: Config
preferences: {}
users:
- name: {{ k8s_username }}
  user:
    client-key-data: {{ k8s_user_certificate }}
    token: {{ k8s_user_token }}
